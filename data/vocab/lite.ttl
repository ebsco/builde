lite:copyrightNote a rdf:Property ;
    rdfs:label "copyright note"@en ;
    rdfs:comment "Note associated with the copyright of an Instance."@en ;
    rdfs:domain lite:CopyrightEvent ;
    rdfs:range rdfs:Literal .

