lite:copyrightDate a rdf:Property ;
    rdfs:label "copyright date"@en ;
    rdfs:comment "Date associated with the copyright of an Instance."@en ;
    rdfs:subPropertyOf lite:date ;
    rdfs:domain lite:CopyrightEvent ;
